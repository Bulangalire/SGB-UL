<link rel="stylesheet" href="{{path('boots')}}">
{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Choix année budgetaire</h1> 
 
   <form action="{{ path('sgb_prevision') }}" method ="post">
        <div class="form-group">
            <select required  name="annees" class="form-control">
            <option value="">--Choisir une année budgetaire </option>
            {% for annee in annees %}
              <option value="{{annee.id}}">{{annee.anneebudget}}</option>
              {% endfor %}
            </select>
            
        </div>
       
        <div class="form-group">
            <button type="submit" class="btn btn-success"> Continuer </button>
            
        </div>
        
   </form>
    
</div>
{% endblock %}
